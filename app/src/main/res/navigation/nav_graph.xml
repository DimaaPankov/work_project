<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/dowlandFragment">

    <fragment
        android:id="@+id/dowlandFragment"
        android:name="com.example2.wallpapercars.view.DowlandFragment"
        android:label="fragment_dowland"
        tools:layout="@layout/fragment_dowland" >
        <action
            android:id="@+id/action_dowlandFragment_to_mainFragment"
            app:destination="@id/mainFragment" />
    </fragment>
    <fragment
        android:id="@+id/mainFragment"
        android:name="com.example2.wallpapercars.view.MainFragment"
        android:label="fragment_main"
        tools:layout="@layout/fragment_main" >
        <action
            android:id="@+id/action_mainFragment_to_deteilFragment"
            app:destination="@id/deteilFragment" />
        <action
            android:id="@+id/action_mainFragment_to_qestionFragment"
            app:destination="@id/qestionFragment" />
    </fragment>
    <fragment
        android:id="@+id/deteilFragment"
        android:name="com.example2.wallpapercars.view.DeteilFragment"
        android:label="fragment_deteil"
        tools:layout="@layout/fragment_deteil" >
        <action
            android:id="@+id/action_deteilFragment_to_mainFragment"
            app:destination="@id/mainFragment" />
        <argument
            android:name="img"
            app:argType="integer" />
    </fragment>
    <fragment
        android:id="@+id/qestionFragment"
        android:name="com.example2.wallpapercars.view.QestionFragment"
        android:label="fragment_qestion"
        tools:layout="@layout/fragment_qestion" >
        <action
            android:id="@+id/action_qestionFragment_to_mainFragment"
            app:destination="@id/mainFragment" />
    </fragment>
</navigation>